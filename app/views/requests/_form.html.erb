<%= layouts/errors, object: @request %>

<%= form_for @request do |f| %>
  <%= f.label :loan_number %>
  <%= f.text_field :loan_number %>
  <%= f.label :borrower_name %>
  <%= f.text_field :borrower_name %>
  <%= f.label :reason %>
  <%= f.text_area :reason, size: "24x6" %>
  <%= f.submit %>
<% end %>
