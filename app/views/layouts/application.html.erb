<!DOCTYPE html>
<html>
  <head>
    <title><%= yield :title %></title>
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
    <%= csrf_meta_tags %>
  </head>
  <body>
    <header>
      <nav>
        <%= yield :menu %>
      </nav>
    </header>
    <main role="main">
      <%= flash[:alert] %>
      <%= flash[:notice] %>
      <%= yield %>
    </main>
  </body>
</html>
